#include <iostream>
using namespace std;

int main() {
  if (30 > 20) {
    cout << "30 is greater than 20";
  }  
  return 0;

  int x = 50;
  int y = 25;
  if (x > y) {
    cout << "x is greater than y";
  }
  return 0;

  int i = 36;
  int j = 19;
  if (i > j) {
    cout << "i is greater than j";
  }  
  return 0;

  int t = 40;
  int u = 20;
  if (t > u) {
    cout << "t is greater than y";
  }  
  return 0;

  int a = 60;
  int b = 30;
  if (a > b) {
    cout << "a is greater than b";
  }  
  return 0;
}
